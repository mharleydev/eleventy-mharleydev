---
layout: base.liquid
---
<article class="post">
    <h1 class="post-title">{{ pageTitle }}</h1>
    <div class="post-date">Published {{ date | date: "%B %d, %Y" }}</div>
    <img class="post-header-image" src="{{ postHeaderImageUrl }}" alt="Header image">
    <section class="post-content">
    {{ content }}
    </section>
</article>
